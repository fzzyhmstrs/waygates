  "authors": [
    "fzzyhmstrs"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/amethyst-imbuement",
    "issues": "https://github.com/fzzyhmstrs/ai/issues",
    "sources": "https://github.com/fzzyhmstrs/ai"
  },
  "license": "MIT License",
  "icon": "assets/waygates/icon.png",
